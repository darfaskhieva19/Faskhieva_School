<Window x:Class="Faskhieva_School.ExitAdmin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Faskhieva_School"
        mc:Ignorable="d"
        Title="ExitAdmin" Height="220" Width="500">
    <Grid>
        <StackPanel Margin="15">
            <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 25 0 0">
                <TextBlock Name="tbVhod" Text="Вы уверены, что хотите выйти из режима администратора?"/>
            </StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center"  Margin="0 35 0 0">
                <Button Name="btnEnter" Content="Подтвердить" Click="btnEnter_Click"  Template="{StaticResource Btn2}"/>
                <Button Name="btnOtm" Content="Отменить" Click="btnOtm_Click"  Template="{StaticResource Btn2}"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
